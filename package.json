{
  "name": "vanilla-icon-picker-virtualized",
  "version": "0.1.0",
  "description": "Icon picker - Vanilla JS icon picker with virtualization",
  "keywords": [
    "ux",
    "vanilla",
    "Fontawesome",
    "icon",
    "icon-picker"
  ],
  "main": "./dist/icon-picker.min.js",
  "module": "./dist/icon-picker.esm.js",
  "source": "./src/index.js",
  "exports": {
    ".": {
      "import": "./dist/icon-picker.esm.js",
      "require": "./dist/icon-picker.min.js",
      "default": "./dist/icon-picker.min.js"
    },
    "./style.css": "./dist/icon-picker.css",
    "./themes/bootstrap-5.min.css": "./dist/themes/bootstrap-5.min.css",
    "./themes/default.min.css": "./dist/themes/default.min.css"
  },
  "typings": "./types/IconPicker.d.ts",
  "author": "Simon <simon@appolo.fr>",
  "contributors": [
    "RikWaelen",
    "RobinErtz"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "webpack serve --progress --mode=development --config=config/webpack.dev.js",
    "build": "node ./config/webpack.prod.js"
  },
  "homepage": "https://github.com/RikWaelen/vanilla-icon-picker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RikWaelen/vanilla-icon-picker.git"
  },
  "bugs": {
    "url": "https://github.com/RikWaelen/vanilla-icon-picker/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "mini-css-extract-plugin": "^2.6.0",
    "sass": "^1.49.10",
    "sass-loader": "^12.6.0",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.2",
    "webpack-fix-style-only-entries": "^0.6.1",
    "webpack-remove-empty-scripts": "^1.0.4"
  }
}
